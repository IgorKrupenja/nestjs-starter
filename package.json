{
  "name": "nestjs-starter",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "nest build",
    "dev": "NODE_ENV=development nest start  --watch",
    "format": "prettier \"**/*.{ts,json,yml}\" --write",
    "format:check": "prettier \"**/*.{ts,json,yml}\" --check",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix --max-warnings 0",
    "lint:check": "eslint \"{src,test}/**/*.ts\" --max-warnings 0",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production npx prisma migrate deploy && node dist/main.js",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.10",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.10",
    "@nestjs/platform-express": "^11.1.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "compression": "^1.8.1",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.10",
    "@swc/cli": "0.7.9",
    "@swc/core": "^1.15.7",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/eslint-plugin": "^1.6.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "unplugin-swc": "^1.5.9",
    "vitest": "^4.0.16",
    "vitest-mock-extended": "^3.1.0"
  },
  "engines": {
    "node": "22.21.0",
    "pnpm": "10.26.1"
  },
  "packageManager": "pnpm@10.26.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@swc/core",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
