{
  "name": "nestjs-starter",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "format": "prettier \"{src,test}/**/*.{ts,json,yml}\" --write",
    "format:check": "prettier \"{src,test}/**/*.{ts,json,yml}\" --check",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix --max-warnings 0",
    "lint:check": "eslint \"{src,test}/**/*.ts\" --max-warnings 0",
    "test": "vitest",
    "test:run": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "build": "nest build",
    "dev": "NODE_ENV=development nest start  --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production npx prisma migrate deploy && node dist/main.js"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.11",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.11",
    "@nestjs/platform-express": "^11.0.11",
    "@prisma/client": "^6.4.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.0",
    "nestjs-prisma": "^0.25.0",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.22.0",
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "^11.0.2",
    "@nestjs/testing": "^11.0.11",
    "@swc/cli": "0.6.0",
    "@swc/core": "^1.11.8",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.10",
    "@vitest/coverage-v8": "^3.0.8",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "prisma": "^6.4.1",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0",
    "unplugin-swc": "^1.5.1",
    "vitest": "^3.0.8",
    "vitest-mock-extended": "^3.0.1"
  },
  "prisma": {
    "seed": "ts-node prisma/seeders/seed.ts"
  },
  "engines": {
    "node": "22.14.0",
    "pnpm": "10.6.1"
  },
  "packageManager": "pnpm@10.6.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@prisma/client",
      "@prisma/engines",
      "@swc/core",
      "esbuild",
      "prisma"
    ]
  }
}
